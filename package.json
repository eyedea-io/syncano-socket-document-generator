{
  "name": "syncano-socket-document-generator",
  "version": "0.1.0",
  "description": "Generate documents from templates.",
  "repository": "https://github.com/eyedea-io/syncano-socket-document-generator.git",
  "license": "MIT",
  "scripts": {
    "test": "yarn && npm run test:lint && npm run test:e2e:coverage",
    "test:e2e-single": "cross-env NODE_ENV=test mocha -s 1800 --reporter spec --require babel-register --timeout 4000",
    "test:e2e-single:coverage": "nyc --produce-source-map=true cross-env NODE_ENV=test mocha -s 1800 --reporter spec --require babel-register --timeout 4000",
    "test:e2e": "npm run test:e2e-single",
    "test:e2e:coverage": "nyc --produce-source-map=true npm run test:e2e",
    "test:lint": "standard --fix"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "chai": "^4.1.2",
    "mocha": "^3.5.3",
    "standard": "^10.0.3",
    "syncano-test": "^0.1.10"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "marked": "^0.3.6",
    "mustache": "^2.3.0",
    "syncano-server": "beta"
  },
  "babel": {
    "plugins": [
      "syntax-object-rest-spread",
      "transform-object-rest-spread"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "7"
          }
        }
      ]
    ]
  }
}
